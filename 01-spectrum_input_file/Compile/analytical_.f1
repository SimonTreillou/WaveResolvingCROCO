      integer IstrR,IendR,JstrR,JendR

      integer IstrU


      if ((istr.eq.1 .and. .not.WEST_INTER)) then

        IstrR=Istr-1
        IstrU=Istr+1

      else
        IstrR=Istr

        IstrU=Istr

      endif

      if ((iend.eq.Lmmpi .and. .not.EAST_INTER)) then

        IendR=Iend+1

      else
        IendR=Iend
      endif

      if ((jstr.eq.1.and. .not.SOUTH_INTER)) then

        JstrR=Jstr-2

      else
        JstrR=Jstr

      endif

      if ((jend.eq.Mmmpi .and. .not.NORTH_INTER)) then

        JendR=Jend+2

      else
        JendR=Jend
      endif

!





      if ((istr.eq.1 .and. .not.WEST_INTER)) then
        do k=1,N
          do j=Jstr,Jend
            unbqbry_west(j,k)=0.5*(Hz(0,j,k)+Hz(1,j,k))
     &                                   *u(1,j,k,nrhs)
          enddo
          do j=Jstr,Jend
            vnbqbry_west(j,k)=0.5*(Hz(0,j,k)+Hz(0,j-1,k))
     &                                   *v(0,j,k,nrhs)
          enddo

          do j=IstrR,IendR
            wnbqbry_west(j,k)=0.
            rnbqbry_west(j,k)=0.
          enddo

        enddo
      endif

